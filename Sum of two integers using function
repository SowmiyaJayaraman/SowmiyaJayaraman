# include<stdio.h>
void main()
{
int n1,n2,sum;
printf("enter the number");
scanf("%d%d", &n1,&n2);
sum=n1+n2;
printf("sum of two integer :%d", sum);
}
***************************************
output:
enter the number 12
4
sum of two integer 16
**************************************
